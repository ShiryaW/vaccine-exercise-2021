{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "node",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "semi": [2, "always"],
    "node/no-unpublished-require": "off",
    "node/no-unsupported-features/node-builtins": ["error", {
      "version": ">=11.14.0"
    }]
  }
}
